<ContentDialog x:ConnectionId='1'
    x:Class="Covid19Analysis.View.DuplicateEntryContentDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Replace or Skip Files"
    PrimaryButtonText="Replace"
    SecondaryButtonText="Skip"
                                                         
                                                             >

    <Grid Height="150" Width="500">
        <TextBlock x:ConnectionId='2' x:Name="subtitleTextBox" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Currently No Duplicate Items" Margin="0,25,0,100" Width="499" Grid.ColumnSpan="2"/>
        <TextBlock x:ConnectionId='3' x:Name="contentTextBox" HorizontalAlignment="Center" TextWrapping="Wrap" Text="Insert Item Here" Margin="0,66,0,-6" Width="501"/>
        <CheckBox x:ConnectionId='4' x:Name="repeatActionForAll" Content="Complete the Following Action for the Remaining Items" Margin="0,118,129,0" VerticalAlignment="Top" HorizontalAlignment="Right"                             Height="32" Width="371"                               />
    </Grid>
</ContentDialog>

